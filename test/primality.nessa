fn is_prime(n: &&Number) -> Bool {
    if n <= 1 {
        return false;
    }
    
    let i: Number = 1;

    while i < n - 1 {
        i = i + 1;

        if n % i == 0 {
            return false;
        }
    }

    return true;
}

let array: Array<Number> = arr<Number>();
let it: Number = 0;

while it < 1000 {
    if is_prime(it) {
        array.push<Number>(it.deref<Number>());
    }

    it = it + 1;
}

if array.len() != 168 {
    panic("Invalid number of primes");
}