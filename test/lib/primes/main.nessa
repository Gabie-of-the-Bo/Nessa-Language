module primes [1.0]

import class Range from range;
import fn { iterator, next, is_consumed } from range;

fn is_prime(n: Number) -> Bool {
    if n <= 1 {
        return false;
    }
    
    let i: Number = 1;

    for i in Range(1, 1, n - 1) {
        i = i + 1;

        if n % i == 0 {
            return false;
        }
    }

    return true;
}
